    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unicorn": "53.0.0",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@clack/prompts": "0.7.0",
